                                                <th>Subquality</th>
                                                {{!-- <th>Dana Class</th> --}}
                                                {{!-- <th>Dana Rate</th> --}}
                                                <th>HSN Code</th>
                                                {{!-- <th>APMC</th>
                                                <th>Dana Dalai</th>
                                                <th>Dalali</th>
                                                <th>Disc %</th>
                                                <th>Tax Category</th>
                                                <th>Rate Based On</th>
                                                <th>Packing</th>
                                                <th>Laga Paisa</th>
                                                <th>Unit</th>
                                                <th>Container Code</th>
                                                <th>Container Charges</th>
                                                <th>Entry Tax</th> --}}
                                            </tr>
                                        </tfoot>
                                    </table>
                                </div>
                            </div>
                            {{!-- {{/scuritylogin}} --}}

                        </div>
                        <!-- END PAGE CONTENT INNER -->
                    </div>
                </div>
                <!-- END PAGE CONTENT BODY -->
                <!-- END CONTENT BODY -->
            </div>
            <!-- END CONTENT -->
        </div>
        <!-- END CONTAINER -->
    </div>
</div>
