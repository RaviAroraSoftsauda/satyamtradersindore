<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <link rel="stylesheet" href="css/head.css">
  <link href="css/bootstrap.min.css" rel="stylesheet" type="text/css" />
  <script src="js/head.js"></script>
  <title>Login</title>
</head>
<body>

  
<div class="page-wrapper-row full-height">
    <div class="page-wrapper-middle">
        <!-- BEGIN CONTAINER -->
        <div class="page-container">
            <!-- BEGIN CONTENT -->
            <div class="page-content-wrapper">
                <!-- BEGIN PAGE CONTENT BODY -->
                <div class="page-content">
                    <div class="container-fluid">
                        <!-- BEGIN PAGE CONTENT INNER -->
                        <div class="page-content-inner">
                            <div class="login">
                                <img src="/img/logo-long.png" alt="" width="240" class="center-block longLogo">
                              <form class="form-horizontal" action="/userright/login" method="post">
                                  <h3 class="text-center head">Login</h3>
                                  <hr>
                                  <div class="row">
                                    <div class="form-group form-md-line-input clearfix">
                                        <label for="" class="control-label col-md-3">Username</label>
                                        <div class="col-md-9">
                                          <input type="text" name="usrnm" class="form-control" id="uninput" autofocus >
                                        </div>
                                    </div>
                                  </div>
                                  {{!-- end of row --}}
                                  <div class="row">
                                    <div class="form-group form-md-line-input clearfix">
                                        <label for="" class="control-label col-md-3">Password</label>
                                        <div class="col-md-9">
                                          <input type="password" name="usrpwd" class="form-control" id="passinpput" autocomplete="off">
                                        </div>
                                    </div>
                                  </div>
                                  {{!-- end of row --}}
                                  <div class="row">
                                    <div class="form-group form-md-line-input clearfix">
                                        <div class="col-md-9 col-md-push-3">
                                          <button type="submit" class="btn btn-primary">Login</button>
                                        </div>
                                    </div>
                                  </div>
                                  {{!-- end of row --}}
                              </form>
                            </div>
                        </div>                        
                        <!-- END PAGE CONTENT INNER -->
                    </div>
                </div>
                <!-- END PAGE CONTENT BODY -->
                <!-- END CONTENT BODY -->
            </div>
            <!-- END CONTENT -->
        </div>
        <!-- END CONTAINER -->
    </div>
</div>
  
</body>
</html>