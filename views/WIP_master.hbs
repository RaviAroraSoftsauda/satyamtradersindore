{{> header_new}}
{{> top_bar_new}}
{{> menu_new}}
<div class="page-wrapper-row full-height">
    <div class="page-wrapper-middle">
        <!-- BEGIN CONTAINER -->
        <div class="page-container">
            <!-- BEGIN CONTENT -->
            <div class="page-content-wrapper">
                <!-- BEGIN CONTENT BODY -->
                <!-- BEGIN PAGE HEAD-->
                <div class="page-head">
                    <div class="container-fluid">
                        <!-- BEGIN PAGE TITLE -->
                        <div class="page-title">
                            <h1>{{compnm}} -{{divnm}}- {{dateformat  sdate 'DD/MM/YYYY'}} - {{dateformat edate
                                'DD/MM/YYYY'}} {{usrnm}}
                                <small>

                                    <!-- BEGIN PAGE BREADCRUMBS -->
                                    <ul class="page-breadcrumb breadcrumb">
                                        <li>
                                            <a href="/userright/index">Home</a>
                                            <i class="fa fa-circle"></i>
                                        </li>
                                        <li>
                                            Party List
                                        </li>
                                    </ul>
                                    <!-- END PAGE BREADCRUMBS -->
                                </small>
                            </h1>
                        </div>
                        <!-- END PAGE TITLE -->
                    </div>
                </div>
                <!-- END PAGE HEAD-->
                <!-- BEGIN PAGE CONTENT BODY -->
                <div class="page-content">
                    <div class="container-fluid">
                        <!-- BEGIN PAGE CONTENT INNER -->
                        <div class="page-content-inner">
                             {{!-- {{#scuritylogin   security 'Product Master' 'V'}} --}}
                            <div class="panel panel-primary">
                                <div class="panel-heading">
                                    WIP List
                                    <div class="misc pull-right">
                                        {{!-- {{#scuritylogin   security 'Product Master' 'A'}} --}}
                                        <div class="buttons">
                                            <a href="/WIP_master/WIP_master_add">
                                                <button class="btn btn-link" type="button"><i class="fa fa-plus"></i>Add
                                                WIP Master</button>
                                            </a>
                                        </div>
                                        {{!-- {{/scuritylogin}} --}}
                                    </div>
                                </div>
                                <div class="panel-body">
                                    <table id="product_detail_table" class="product_detail_table table table-striped table-bordered table-hover">
                                        <thead>
                                            <tr>
                                        <th>Action</th>
                                        <th>Sno.</th>
                                        <th>Product</br> Code</th>
                                        <th>Product</br> Description</th>
                                        <th>Model</th>
                                        <th>Brand</th>
                                        <th>Unit</th>
                                        </tr>
                                        </thead>
                                        <tbody>
                                            {{#each product_mast as |value key|}}
                                            <tr>
                                                <td>
                                                    <div class="btn-group dropdown">
                                                        <button class="btn btn-xs blue dropdown-toggle" type="button"
                                                            data-toggle="dropdown" aria-expanded="false"> Actions
                                                            <i class="fa fa-angle-down"></i>
                                                        </button>
                                                        <ul class="dropdown-menu pull-left" role="menu" style="width:150px">
                                                            {{!-- {{#scuritylogin   ../security 'Product Master' 'E'}} --}}
                                                            <li>
                                                                <a href="/WIP_master/edit_WIP_master/{{this._id}}"
                                                                    data-id="{{this._id}}">
                                                                    <i class="icon-pencil"></i> Edit </a>
                                                            </li>
                                                            {{!-- {{/scuritylogin}} --}}
                                                            <li class="divider"> </li>
                                                             {{!-- {{#scuritylogin   ../security 'Product Master' 'D'}} --}}
                                                            <li>
                                                                <a href="javascript:void(0);" class="delbutton delete_WIP_master"
                                                                    data-id="{{this._id}}">
                                                                    <i class="icon-trash"></i> Delete
                                                                </a>
                                                            </li>
                                                            {{!-- {{/scuritylogin}} --}}
                                                        </ul>
                                                    </div>
                                                </td>
                                                <td>{{sum @index 1}}</td>
                                                <td>{{this.Fg_PrCode}}</td>
                                                <td>{{this.Fg_Des}}</td>
                                                <td>{{this.Fg_Model.Description}}</td>
                                                <td>{{this.Fg_Brand.Description}}</td>
                                                <td>{{this.Fg_Unit.SKUName}}</td>
                                            </tr>
                                            {{/each}}
                                        </tbody>
                                        <tfoot>
                                            <tr>
                                    <th  class="excludesearch">Action</th>
                                        <th>Sno.</th>
                                        <th>Product</br> Code</th>
                                        <th>Product</br> Description</th>
                                        <th>Model</th>
                                        <th>Brand</th>
                                        <th>Unit</th>
                                            </tr>
                                        </tfoot>
                                    </table>
                                </div>
                            </div>
                            {{!-- {{/scuritylogin}} --}}

                        </div>
                        <!-- END PAGE CONTENT INNER -->
                    </div>
                </div>
                <!-- END PAGE CONTENT BODY -->
                <!-- END CONTENT BODY -->
            </div>
            <!-- END CONTENT -->
        </div>
        <!-- END CONTAINER -->
    </div>
</div>
{{> footer_new}}
{{> scripts_new}}