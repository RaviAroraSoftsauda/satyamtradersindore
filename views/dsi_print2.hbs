{{> header_new}}
{{> top_bar_new}}
{{> menu_new}}
 <style>
body { 
  font-size: 10px; 
}
@media print {
    .borddiv{
        border: 5em;
    }
    table tr.tx_invoice > td{
        background-color: #ccc4c4 !important;
    }
    td.tdcolor{
        background-color: #ccc4c4 !important;
    }
    @page :bottom {
     margin: 5em;
    }
    @page :right {
     margin: 0.1cm;
    }
    html, body{
      height:100%;
      width:100%;
    }
    table td{   font-size: 1.2em; 
                padding: 1px;
                height: 100%;
            }
    table th{   font-size: 1.2em; 
                padding: 1px;
                height: 100%;
    }
    table { border-collapse: collapse; }
    table {
        page-break-inside: auto !important;
        overflow: hidden;
        font-size: 1em;
        margin-top: 2em;
        margin-right: 2%;
        margin-bottom: 1%;
        margin-left: 5%;
        width: 93%;
    }
    tr    { page-break-inside:avoid; page-break-after:auto }
    thead { display:table-header-group }
    tfoot { display:table-footer-group }
    
}
  
    td.imgborder1{
        border-right: none;
    }
    td.imgborder2{
        border-left: none;
    }
td.noBorder {
        border-top: none;
    }
    td.nodBorder {
        border-bottom: none;
    }
td.notdBorder {
    border-bottom: none;
    border-top: none;
}
.container{
    width:fit-content;
    margin-right: auto;
    margin-left: auto;
	height:auto;
}

h2 {
  text-align: center;
  padding: 20px 0;
}
table caption {
	padding: .5em 0;
}
th, td {
  padding: 5px;
}
p {
    padding: 0%;
    width:50%;
    display:inline-block;
}
.sizecheck{
    height: 20px;
}
#invoiceBill td, #invoiceBill th {
  border: 1px solid black;
}
#invoiceBill{
  border: 3px solid black;
  border-color: black;
}

</style>
<div class="page-wrapper-row full-height">
    <div class="page-wrapper-middle">
        <div class="page-container">
            <div class="page-content-wrapper">
                <div class="page-head">
                    <div class="container-fluid">
                        <div class="page-title">
                           <h1>{{compnm}} -{{divnm}}- {{dateformat  sdate 'DD/MM/YYYY'}} - {{dateformat edate
                                'DD/MM/YYYY'}} {{usrnm}}
                                <small>
                                    <ul class="page-breadcrumb breadcrumb">
                                        <li>
                                            <a href="/userright/index">Home</a>
                                            <i class="fa fa-circle"></i>
                                        </li>
                                    </ul>
                                </small>
                            </h1>
                        </div>
                    </div>
    </div>
    <div style="width: 95%;margin-left: 5%;" align="center" >
	   <table  align="center" id="invoiceBill">
		 <thead>
            <tr>
                <td rowspan="5" colspan="2" style="border-right: none" valign="top" class="imgborder1"><img src="logo/gg.jpg" alt="logo" class="logo-default" style="widows:80px;height: 80px"></td>
                <td colspan="8" rowspan="5" class="text-center imgborder2" style="width: 100px;">
                    <strong style="font-size:25px; align-items: center;font-family: Times New Roman, Times, serif;">{{division.div_mast}}</strong></br>
                    <strong  style="font-size:16px;align-items: center;font-family: Times New Roman, Times, serif;">{{division.ac_add2}}</strong></br>
                    <strong  style="font-size:16px;align-items: center;font-family: Times New Roman, Times, serif;">Tel: {{division.ac_pho}}</strong></br>
                    <strong  style="font-size:16px;align-items: center;font-family: Times New Roman, Times, serif;">GSTIN: {{division.ac_gstin}}</strong></br>
                    <strong  style="font-size:16px;align-items: center;font-family: Times New Roman, Times, serif;">CIN  : {{division.ac_cin}}</strong></br>
                </td>
            </tr>
            <tr>
                <td style="padding-right: 0px;" colspan="4"><input type="checkbox" class="sizecheck">Original for Receipient</div></td>
            </tr>
            <tr>
                <td style="padding-right: 0px" colspan="4"><input type="checkbox" class="sizecheck">Duplicate for Transport</td>
            </tr>
            <tr>
                <td style="padding-right: 0px;" colspan="4"><input type="checkbox" class="sizecheck">Triplicate for Supplier</td>
            </tr>
            <tr>
                <td style="height: 5em; border-left: none" colspan="4"></td>
            </tr>
            <tr style="background-color: #ccc4c4" class="tx_invoice">
				<td colspan="14" align="left" margin="1%" class="text-center" bgcolor="#ccc4c4">
                    <b  style="font-size:26px;align-items: center;font-family: Times New Roman, Times, serif;">Tax Invoice</b></br>
                </td>
            </tr>
          <tr>
              <td colspan="7"><strong>Invoice No</strong>:&nbsp{{dsimast.vouc_code}}</td>
              <td colspan="7"><strong>Transport Mode</strong>:&nbsp{{dsimast.Transport}}</td>
          </tr>
          <tr>
              <td colspan="7"><strong>Invoice Date</strong>:&nbsp{{dateformat dsimast.so_datemilisecond 'DD/MM/YYYY'}}</td>
              <td colspan="7"><strong>Vehicle No</strong>:&nbsp{{dsimast.Vehicle}}</td>
          </tr>
          <tr>
              <td colspan="7"><strong>Reverse Charge (Y/N)</strong>:&nbsp<strong>N</strong></td>
              <td colspan="7"><strong>Date of Supply</strong>:&nbsp{{dateformat dsimast.so_datemilisecond 'DD/MM/YYYY'}}</td>
          </tr>
          <tr>
              <td colspan="5"><strong>State</strong>: &nbsp{{division.ac_state.StateName}}</td>
              <td><strong>Code</strong></td>
              <td>{{division.ac_state.StateCode}}</td>
              <td colspan="7"><strong>Place of Supply</strong>:&nbsp
                {{#ifCond dsimast.Ship_party ''}}{{dsimast.buy_cus_name.CityName.CityName}}{{/ifCond}}
                {{#ifCondNot dsimast.Ship_party ''}}{{dsimast.Ship_party_City.CityName}}{{/ifCondNot}}
              </td>
          </tr>
          <tr id="name" class="tx_invoice">
				<td colspan="7" align="center" width="550PX"  bgcolor="#ccc4c4" >
                    <b  style="font-size:12px;align-items: center;font-family: Times New Roman, Times, serif;">Bill To Party</b></br>
                </td>
			    <td colspan="7" align="center" width="550PX"  bgcolor="#ccc4c4" >
                    <b  style="font-size:12px;align-items: center;font-family: Times New Roman, Times, serif;">Ship To Party</b></br>
                </td>
          </tr>
          <tr>
              <td colspan="7"><strong>Name</strong>:&nbsp{{dsimast.buy_cus_name.ACName}}</td>
              <td colspan="7"><strong>Name</strong>:&nbsp{{dsimast.Ship_party}}</td>
          </tr>
          <tr>
              <td colspan="7"><strong>Address</strong>&nbsp{{dsimast.buy_cus_name.Address1}}</td>
              <td colspan="7"><strong>Address</strong>:&nbsp{{dsimast.Ship_party_add}}</td>
          </tr>
          <tr>
              <td colspan="7"><strong>GSTIN</strong>:&nbsp{{dsimast.buy_cus_name.GSTIN}}</td>
              <td colspan="7"><strong>GSTIN</strong>:&nbsp{{dsimast.Ship_party_gst}}</td>
          </tr>
          <tr>
              <td colspan="2" style="border-left-color: black"><strong>City</strong>:&nbsp{{dsimast.buy_cus_name.CityName.CityName}}</td>
              <td colspan="3"><strong>State</strong>:&nbsp{{dsimast.buy_cus_name.CityName.StateName.StateName}}</td>
              <td><strong>Code</strong></td>
              <td>{{dsimast.buy_cus_name.CityName.StateName.StateCode}}</td>
              <td colspan="5"><strong>State</strong>:&nbsp{{dsimast.Ship_party_State.StateName}}</td>
              <td colspan="2"><strong>Code</strong>:&nbsp{{dsimast.Ship_party_State.StateCode}}</td>
              {{!-- <td></td> --}}
          </tr>
          <tr id="name">
				<th rowspan="2"  style="width: 40px;">S.</br>NO</th>
				<th colspan="2" rowspan="2"  style="width: 150px;" class="text-left DESCRIPTION">DESCRIPTION OF GOODS</th>
				<th rowspan="2"  class="text-right" style="width: 40px;">HSN/SAC</th>
                <th rowspan="2"  class="text-right TaxableValue" style="width: 40px;">Qty</th>
                <th rowspan="2" class="text-right TaxableValue" style="width: 50px;">Rate</th>
                <th rowspan="2" class="text-right TaxableValue" style="width: 50px;">Amount</th>
                <th rowspan="2" style="width:40px;" class="text-right TaxableValue">Taxable Value </th>
                <th colspan="2" style="width: 60px;"  class="text-center">SGST</th>
                <th colspan="2" style="width: 60px;"  class="text-center">CGST</th>
                <th rowspan="2" class="text-right TaxableValue" style="width:40px;" colspan="2">Total</th>
		  </tr>
          <tr>
                <th style="width: 20px;">Rate</th>
                <th style="width: 40px;">Amount</th>
                <th style="width: 20px;">Rate</th>
                <th style="width: 40px;">Amount</th>
          </tr> 
           </thead>	
           <tbody>
		{{#each dsimast as |promastvalue key|}}
           {{#each promastvalue.sales_or_group as |groupvalue key|}}
            <tr id="name">
				<td class="text-center" style="width: 30px">{{sum @index 1}}</td>
				<td colspan="2" class="text-left DESCRIPTION" style="width: 150px"><strong>{{groupvalue.so_disc.Fg_Des}}</strong></td>
				<td class="text-right TaxableValue" style="width: 40px">{{groupvalue.so_disc.Fg_HSN}}</td>
                <td class="text-right TaxableValue">{{groupvalue.so_qty}}</td>
                <td class="text-right TaxableValue">{{groupvalue.ocrate}}</td>
                <td class="text-right TaxableValue">{{groupvalue.so_beforedisamt}}</td>
                <td class="text-right TaxableValue"><strong>{{groupvalue.total_amt}}</strong></td>
                <td class="text-right TaxableValue" ><strong >{{groupvalue.so_gstrate}}%</strong></td>
                <td class="text-right TaxableValue" ><strong>{{groupvalue.so_taxvalue}}</strong></td>
                <td class="text-right TaxableValue" ><strong >{{groupvalue.so_gstrate}}%</strong></td>
                <td class="text-right TaxableValue" ><strong>{{groupvalue.so_taxvalue}}</strong></td>
                <td class="text-right TaxableValue" colspan="2"><strong>{{groupvalue.so_afttaxvalue}}</strong></td>
			</tr> 
               {{/each}}   
             {{/each}}
             {{#each length as |promastvalue key|}}
            <tr>
                <td align="center"></br>&nbsp</td>
                <td colspan="2"></td>
                <td></td>
                <td></td>
                <td></td>
                <td></td>
                <td></td>
                <td></td>
                <td></td>
                <td></td>
                <td></td>
                <td colspan="2"></td>
            </tr>
            {{/each}}   
             
             </tbody>
            <tfoot style="display: table-row-group">
             <tr>
				<td colspan="3" bgcolor="#ccc4c4" align="center" class="Twide"><b  style="font-size:12px;align-items: center;font-family: Times New Roman, Times, serif;">Total</b></br></td>
                <td colspan="1" class="text-right Tqwide"></strong></td>
                <td colspan="1" class="text-right Tqwide"><strong>{{dsimast.tot_sooq}}</td>
                <td colspan="1" class="Trwide"></td>
                <td colspan="1" class="text-right Tawide"><strong>{{dsimast.tot_beforedisamt}}</strong></td>
                <td colspan="1" class="text-right Trwide"><strong>{{dsimast.tot_amtso}}</strong></td>
                <td colspan="1" class="Tqwide"></td>
                <td colspan="1" class="text-right Trwide"><strong>{{dsimast.tot_taxvalue}}</strong></td>
                <td colspan="1" class="Tqwide"></td>
                <td colspan="1" class="text-right Txwide"><strong>{{dsimast.tot_taxvalue}}</strong> </td>
			    <td colspan="2" class="text-right Ttwide"><strong>{{dsimast.grand_total}}</strong> </td>
           </tr> 
                <td colspan="8" class="text-center tdcolor" bgcolor="#ccc4c4"><b  style="font-size:12px;align-items: center;font-family: Times New Roman, Times, serif;">Total Invoice Amount In Words</b></td>
                <td colspan="5" ><b  style="font-size:12px;align-items: center;font-family: Times New Roman, Times, serif;">Total Amount Before Tax</b></td>
				<td class="text-right"><strong>{{dsimast.tot_amtso}}</strong></td>
           </tr> 
            </tr> 
                <td colspan="8" rowspan="2" class="text-center" style="font-size:12px">{{#dsiamountinword dsimast.sales_or_group dsimast.tot_amtso }}{{/dsiamountinword}}only.</td>
                <td colspan="5"  class="text-left">
                    {{#each dsimast.add_details as |value key|}}
                        {{#ifCondNot this.particular_amount 0}}  
                        <b  style="font-size:12px;align-items: left;font-family: Times New Roman, Times, serif;">Add: {{this.particular_add.type_descr}}</b><br>
                        {{/ifCondNot}}
                    {{/each}}
                        {{#each dsimast.less_details as |value key|}}
                        {{#ifCondNot this.particular_amtless 0}}
                        <b  style="font-size:12px;align-items: left;font-family: Times New Roman, Times, serif;">Less: {{this.particular_less.type_descr}}</b></br>
                        {{/ifCondNot}}
                        {{/each}}
                </td>    
				<td class="text-right">
                    {{#each dsimast.add_details as |value key|}}
                        {{#ifCondNot this.particular_amount 0}}  
                        <strong>{{this.particular_amount}}</strong></br>
                        {{/ifCondNot}}
                    {{/each}}
                        {{#each dsimast.less_details as |value key|}}
                        {{#ifCondNot this.particular_amtless 0}}
                        <strong>-{{this.particular_amtless}}</strong></br>
                        {{/ifCondNot}}
                        {{/each}}
                </td>
           </tr>
           </tr> 
                <td colspan="5"><b  style="font-size:12px;align-items: center;font-family: Times New Roman, Times, serif;">Total Amount After Tax</b></td>
                <td class="text-right"><strong>{{dsimast.grand_total}}</strong></td>
           </tr>
            
		    <tr>
                <td colspan="8" class="text-center tdcolor" bgcolor="#ccc4c4"><b  style="font-size:12px;align-items: center;font-family: Times New Roman, Times, serif;">Bank Details</b></td>
                <td colspan="5" bgcolor="#ccc4c4" class="tdcolor"><b  style="font-size:12px;align-items: center;font-family: Times New Roman, Times, serif;">GST On Reverse Charge</b></td>
                <td colspan="1"></td> 
            </tr>
            <tr>
                <td colspan="4"><strong>Bank:</strong>&nbsp{{division.bank1}}</td>
                <td colspan="3" style="border-bottom: none;"></td>
                <td colspan="7" style="border-bottom: none" class="text-left nodBorder">
                    <b style="font-size:10px; align-items: left; font-family: Times New Roman, Times, serif">Certified thet the particulars given above are true & correct</b></br>
                </td>
            </tr>
            <tr>
                <td colspan="4"><strong>Bank A/C:</strong>&nbsp{{division.bank2}}</td>
                <td colspan="3" style="border-bottom: none; border-top: none;"></td>
                <td colspan="7" rowspan="2" class="text-center notdBorder" style="border-bottom: none;border-top: none"><b style="font-size:15px;align-items: center;font-family: Times New Roman, Times, serif">FOR {{division.div_mast}}</b></td>
            </tr>
            <tr>
                <td colspan="4"><strong>Bank IFSC:</strong>&nbsp{{division.bank3}}</td>
            </tr>
             <tr>
                <td colspan="4" style="height: 8em" class="text-center" valign="top"><b  style="font-size:14px;align-items: center;font-family: Times New Roman, Times, serif;">Terms & Conditions</b></td>
                <td colspan="3" style="border-top: none"></td>
                <td colspan="7" valign="bottom" align="center" style="border-top: none;"><b style="font-size:15px;align-items: center;font-family: Times New Roman, Times, serif;">Authorised Signatory</b></br></td>
            </tr>
		</tfoot>
	</table>
    <a class="btn btn-lg blue hidden-print margin-bottom-5" onclick="printinvoice()" id="hideprintbtn" style="margin-top: 2em"> Print
    <i class="fa fa-print"></i></a>
</div>
            </div>
            <!-- END CONTENT -->
        </div>
        <!-- END CONTAINER -->
    </div>
</div>


