{{> header_new}}
{{> top_bar_new}}
{{> menu_new}}
<div class="page-wrapper-row full-height">
    <div class="page-wrapper-middle">
        <!-- BEGIN CONTAINER -->
        <div class="page-container">
            <!-- BEGIN CONTENT -->
            <div class="page-content-wrapper">
                <!-- BEGIN CONTENT BODY -->
                <!-- BEGIN PAGE HEAD-->
                <div class="page-head">
                    <div class="container-fluid">
                        <!-- BEGIN PAGE TITLE -->
                        <div class="page-title">
                            <h1>{{compnm}} -{{divnm}}- {{dateformat  sdate 'DD/MM/YYYY'}} - {{dateformat edate
                                'DD/MM/YYYY'}} {{usrnm}}
                                <small>

                                    <!-- BEGIN PAGE BREADCRUMBS -->
                                    <ul class="page-breadcrumb breadcrumb">
                                        <li>
                                            <a href="/userright/index">Home</a>
                                            <i class="fa fa-circle"></i>
                                        </li>
                                        <li>
                                            Party List
                                        </li>
                                    </ul>
                                    <!-- END PAGE BREADCRUMBS -->
                                </small>
                            </h1>
                        </div>
                        <!-- END PAGE TITLE -->
                    </div>
                </div>
                <!-- END PAGE HEAD-->
                <!-- BEGIN PAGE CONTENT BODY -->
                <div class="page-content">
                    <div class="container-fluid">
                        <!-- BEGIN PAGE CONTENT INNER -->
                        <div class="page-content-inner">
                            <div class="row">
                              
                                <div class="col-md-12">
                                    <div class="panel panel-primary">
                                         <div class="panel-heading" align="right">
                                                Toli Master
                                        </div>
                                        
                                        <div class="panel-body">
                                            <form  enctype="" id="edit_state"
                                                class="validate">
                                                <div class="row">
                                                    <div class="form-group clearfix">
                                                        <label for="" class="control-label col-md-3">Toli Name</label>
                                                        <div class="col-md-9">
                                                            <input class="form-control toli_name" placeholder="Toli Name"
                                                                name="toli_name" type="text" autofocus required/>
                                                            <input class="form-control vhpxstate" id="vhpxstate"
                                                                name="vhpxstate" type="hidden" />
                                                        </div>
                                                    </div>
                                                </div>
                                              
                                                <div class="row">
                                                    <div class="form-group clearfix">
                                                        <label for="" class="control-label col-md-3">Toli Code</label>
                                                        <div class="col-md-9">
                                                            <input class="form-control toli_code" placeholder="Toli Code"
                                                                name="toli_code" type="text">
                                                        </div>
                                                    </div>
                                                </div>
                                            
                                                <div class="row">
                                                    <div class="form-group clearfix">
                                                        <label for="" class="control-label col-md-3"></label>
                                                        <div class="col-md-9">
                                                            <button type="submit" name="save" class="btn btn-success lock-toli">Submit</button>
                                                            <button type="reset" class="btn btn-primary cancelmaster"
                                                              data-form-ad-action="/toli_master/toli_add">Cancel</button>
                                                        </div>
                                                    </div>
                                                </div>
                                            </form>
                                        </div>
                                    </div>
                                    {{!-- {{/scuritylogin}} data-form-ad-action="/state_master/state_add"--}}
                                </div>
                                  <div class="col-md-12">
                                {{!-- {{#scuritylogin   security 'State Master' 'V'}} --}}
                                    <div class="panel panel-primary">
                                        <div class="panel-heading" style="text-align: right">
                                            Toli Master List
                                        </div>
                                        <div class="panel-body">
                                            <div class="table-responsive">
                                                <table id="product_detail_table" class="product_detail_table table table-striped table-bordered table-hover">
                                                    <thead>
                                                        <tr>
                                                            <th>Action</th>
                                                            <th>Sno.</th>
                                                            <th>Toli Name</th>
                                                            <th>Toli Code</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        {{#each  toli_master as |value key|}}
                                                        <tr>
                                                             <td>
                                                                <div class="btn-group dropdown">
                                                                    <button class="btn btn-xs blue dropdown-toggle"
                                                                        type="button" data-toggle="dropdown"
                                                                        aria-expanded="false"> Actions
                                                                        <i class="fa fa-angle-down"></i>
                                                                    </button>
                                                                    <ul class="dropdown-menu pull-left" role="menu" style="width:150px">
                                                                        {{!-- {{#scuritylogin   ../security 'State Master' 'E'}} --}}
                                                                        <li>
                                                                            <a href="javascript:void(0);" class="edit_toli_btn"
                                                                                data-id="{{this._id}}">
                                                                                <i class="icon-pencil"></i> Edit </a>
                                                                        </li>
                                                                        {{!-- {{/scuritylogin}} --}}
                                                                        <li class="divider"> </li>
                                                                         {{!-- {{#scuritylogin   ../security 'State Master' 'D'}} --}}
                                                                        <li>
                                                                            <a href="javascript:void(0);" class="delete-toli"
                                                                                data-id="{{this._id}}">
                                                                                <i class="icon-trash"></i> Delete
                                                                            </a>
                                                                        </li>
                                                                        {{!-- {{/scuritylogin}} --}}
                                                                    </ul>
                                                                </div>
                                                            </td>
                                                            <td>{{sum @index 1}}</td>
                                                            <td>{{this.ToliName}}</td>
                                                            <td>{{this.ToliCode}}</td>
                                                        </tr>
                                                        {{/each}}
                                                    </tbody>
                                                    <tfoot>
                                                        <tr>
                                                            <th class="excludesearch"></th>
                                                            <th>Sno.</th>
                                                            <th>Toli Name</th>
                                                            <th>Toli Code</th>
                                                        </tr>
                                                    </tfoot>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                   {{!-- {{/scuritylogin}} --}}
                                </div>
                            </div>


                        </div>
                        <!-- END PAGE CONTENT INNER -->
                    </div>
                </div>
                <!-- END PAGE CONTENT BODY -->
                <!-- END CONTENT BODY -->
            </div>
            <!-- END CONTENT -->
        </div>
        <!-- END CONTAINER -->
    </div>
</div>
{{> footer_new}}
{{> scripts_new}}