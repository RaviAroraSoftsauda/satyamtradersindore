<div class="page-wrapper-row full-height">
    <div class="page-wrapper-middle">
        <!-- BEGIN CONTAINER -->
        <div class="page-container">
            <!-- BEGIN CONTENT -->
            <div class="page-content-wrapper">
                <!-- BEGIN CONTENT BODY -->
                <!-- BEGIN PAGE HEAD-->
                <div class="page-head">
                    <div class="container-fluid">
                        <!-- BEGIN PAGE TITLE -->
                        <div class="page-title">
                            <h1>{{compnm}} -{{divnm}}- {{dateformat  sdate 'DD/MM/YYYY'}} - {{dateformat edate
                                'DD/MM/YYYY'}} {{usrnm}}
                                <small>

                                    <!-- BEGIN PAGE BREADCRUMBS -->
                                    <ul class="page-breadcrumb breadcrumb">
                                        <li>
                                            <a href="/userright/index">Home</a>
                                            <i class="fa fa-circle"></i>
                                        </li>
                                        <li>
                                            Party Type List
                                        </li>
                                    </ul>
                                    <!-- END PAGE BREADCRUMBS -->
                                </small>
                            </h1>
                        </div>
                        <!-- END PAGE TITLE -->
                    </div>
                </div>
                <!-- END PAGE HEAD-->
                <!-- BEGIN PAGE CONTENT BODY -->
                <div class="page-content">
                    <div class="container-fluid">
                        <!-- BEGIN PAGE CONTENT INNER -->
                        <div class="page-content-inner">
                            <div class="row">

                               
                                <div class="col-md-12">
                                    <div class="panel panel-primary">
                                        <div class="panel-heading" id="formheading">
                                            <span></span> Group Setup
                                        </div>
                                        <div class="panel-body">
                                            {{!-- method="POST" action="/group_setup_mast/group_setup_mast_add" --}}
                                            <form  enctype=""></form>
                                               
                                                <div class="row">
                                                    <div class="form-group clearfix">
                                                        <label for="" class="control-label col-md-3">Group</label>
                                                        <div class="col-md-9">
                                                            <input class="form-control groupname" id="group" name="group" type="text" readonly>
                                                            <input class="form-control group_id" name="group_id" id="group_id" type="text" style="display: none">
                                                        </div>
                                                    </div>
                                                </div>
                                                {{!-- end of row --}}
                                                
                                                <div class="row">
                                                    <div class="form-group  form-md-line-input clearfix">
                                                   <label for="" class="control-label col-md-3">Group Name:</label>
                                                        <div class="col-md-9">
                                                        <select class="form-control garry prbank select2" multiple="true" name="garry" id="garry" placeholder="Select Group">
                                                          <option value=""></option>
                                                          
                                                           {{#each g_mast}}
                                                                <option value="{{this._id}}">{{this.GroupName}}</option>
                                                            {{/each}}
                                                        </select>
                                                     </div>
                                                </div>
                                                </div>
                                                {{!-- end of row --}}
                                               
                                                <div class="row">
                                                    <div class="form-group clearfix">
                                                        <label for="" class="control-label col-md-3"></label>
                                                        <div class="col-md-9">
                                                            <button type="submit" name="save" class="btn btn-success update-gsetup">Submit</button>
                                                            <a href="/group_setup_mast/group_setup_mast" class="btn btn-danger">Cancel</a>
                                                        </div>
                                                    </div>
                                                </div>
                                            </form>
                                        </div>
                                    </div>
                                </div>
                                 <div class="col-md-12">
                                    <div class="panel panel-primary">
                                        <div class="panel-heading">
                                          Group Setup List
                                        </div>
                                        <div class="panel-body">
                                            <div class="table-responsive">
                                                <table id="product_detail_table" class="product_detail_table table table-striped table-bordered table-hover">
                                                    <thead>
                                                        <tr>
                                                            <th>Action</th>
                                                            <th>Sno.</th>
                                                            <th>Group</th>
                                                            <th>Group Name</th>
                                                          
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        {{#each g_s_mast as |value key|}}
                                                        <tr>
                                                             <td>
                                                                <div class="btn-group dropdown">
                                                                    <button class="btn btn-xs blue dropdown-toggle"
                                                                        type="button" data-toggle="dropdown"
                                                                        aria-expanded="false"> Actions
                                                                        <i class="fa fa-angle-down"></i>
                                                                    </button>
                                                                    <ul class="dropdown-menu pull-down" role="menu"style="width:150px">
                                                                        <li>
                                                                            <a href="javascript:void(0);" class="edit_group_setup_btn"
                                                                                data-id="{{this._id}}">
                                                                                <i class="icon-pencil"></i> Edit </a>
                                                                        </li>
                                                                        <li class="divider"> </li>
                                                                        {{!-- <li>
                                                                            <a href="javascript:void(0);" class="delete_sub_quality_btn"
                                                                                data-id="{{this._id}}">
                                                                                <i class="icon-trash"></i> Delete
                                                                            </a>
                                                                        </li> --}}
                                                                    </ul>
                                                                </div>
                                                            </td>
                                                            <td>{{sum @index 1}}</td>
                                                            <td>{{this.group}}</td>
                                                            <td>
                                                                {{#each this.garry as |value key|}}
                                                                {{this.GroupName}},&nbsp&nbsp
                                                                {{/each}}
                                                            </td>
                                                            {{!-- <td>{{this.garry}}</td> --}}
                                                            
                                                           
                                                        </tr>
                                                        {{/each}}


                                                    </tbody>
                                                    <tfoot>
                                                        <tr>
                                                            <th class="excludesearch"></th>
                                                            <th>Sno.</th>
                                                            <th>Description</th>
                                                            <th>Code</th>
                                                            
                                                            
                                                        </tr>
                                                    </tfoot>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>


                        </div>
                        <!-- END PAGE CONTENT INNER -->
                    </div>
                </div>
                <!-- END PAGE CONTENT BODY -->
                <!-- END CONTENT BODY -->
            </div>
            <!-- END CONTENT -->
        </div>
        <!-- END CONTAINER -->
    </div>
</div>
