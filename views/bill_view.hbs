<div class="page-wrapper-row full-height">
    <div class="page-wrapper-middle">
        <!-- BEGIN CONTAINER -->
        <div class="page-container">
            <!-- BEGIN CONTENT -->
            <div class="page-content-wrapper">
                <!-- BEGIN CONTENT BODY -->
                <!-- BEGIN PAGE HEAD-->
                <div class="page-head">
                    <div class="container-fluid">
                        <!-- BEGIN PAGE TITLE -->
                        <div class="page-title">
                            <h1>{{compnm}} -{{divnm}}- {{dateformat  sdate 'DD/MM/YYYY'}} - {{dateformat edate
                                'DD/MM/YYYY'}} {{usrnm}}
                                <small>

                                    <!-- BEGIN PAGE BREADCRUMBS -->
                                    <ul class="page-breadcrumb breadcrumb">
                                        <li>
                                            <a href="/userright/index">Home</a>
                                            <i class="fa fa-circle"></i>
                                        </li>
                                        <li>
                                           Bill Register
                                        </li>
                                    </ul>
                                    <!-- END PAGE BREADCRUMBS -->
                                </small>
                            </h1>
                        </div>
                        <!-- END PAGE TITLE -->
                    </div>
                </div>
                <!-- END PAGE HEAD-->
                <!-- BEGIN PAGE CONTENT BODY -->
                <div class="page-content">
                    <div class="container-fluid">
                        <!-- BEGIN PAGE CONTENT INNER -->
                        <div class="page-content-inner">
                            <div class="row">
                                    <div class="advancesearch clearfix">
                               <div class="col-md-10">
                                        <div class="row">
                                            <div class="col-md-3">
                                                <div class="form-group clearfix">
                                                    <label for="" class="control-label col-md-3">Bill</label>
                                                    <div class="col-md-9">
                                                        <input type="text" class="form-control" onkeyup="getbill()" name="billno" id="billno" autocomplete="off" />
                                                    </div>
                                                </div>
                                                {{!-- end of form-group clearfix --}}
                                            </div>
                                             <div class="col-md-3">
                                                <div class="form-group clearfix">
                                                    <label for="" class="control-label col-md-3">Date (M/d/y)</label>
                                                    <div class="col-md-9" id="bldate">
                                                        <input type="text" name="bldate" id="bldate" class="form-control">
                                                    </div>
                                                </div>
                                            </div>
                                              <div class="col-md-6">
                                                <div class="form-group clearfix">
                                                    <label for="" class="control-label col-md-3">Company</label>
                                                    <div class="col-md-9" class="company">
                                                        <input type="text" class="form-control" name="copmpany"  autocomplete="off" />
                                                    </div>
                                                </div>
                                                {{!-- end of form-group clearfix --}}
                                            </div>
                                        </div>
                                        {{!-- /// --}}
                                         <div class="row">
                                            <div class="col-md-3">
                                                <div class="form-group clearfix">
                                                    <label for="" class="control-label col-md-3">Bill From</label>
                                                    <div class="col-md-9" id="blfromdate">
                                                        <input type="text" class="form-control" name="blfromdate" autocomplete="off" />
                                                    </div>
                                                </div>
                                                {{!-- end of form-group clearfix --}}
                                            </div>
                                             <div class="col-md-3">
                                                <div class="form-group clearfix">
                                                    <label for="" class="control-label col-md-3">To</label>
                                                    <div class="col-md-9" id="bltodate">
                                                        <input type="text" name="bltodate" id="bltodate" class="form-control">
                                                    </div>
                                                </div>
                                            </div>
                                              <div class="col-md-6">
                                                <div class="form-group clearfix">
                                                    <label for="" class="control-label col-md-3">Party</label>
                                                    <div class="col-md-9" id="partyname">
                                                        <input type="text" class="form-control" name="partyname"  autocomplete="off" />
                                                    </div>
                                                </div>
                                                {{!-- end of form-group clearfix --}}
                                            </div>
                                        </div>
                                </div>
                                </div>
                            </div>
                            <div class="billview_detail_body"></div>
                        </div>
                        <!-- END PAGE CONTENT INNER -->
                    </div>
                </div>
                <!-- END PAGE CONTENT BODY -->
                <!-- END CONTENT BODY -->
            </div>
            <!-- END CONTENT -->
        </div>
        <!-- END CONTAINER -->
    </div>
</div>

