<div class="page-wrapper-row full-height">
    <div class="page-wrapper-middle">
        <!-- BEGIN CONTAINER -->
        <div class="page-container">
            <!-- BEGIN CONTENT -->
            <div class="page-content-wrapper">
                <!-- BEGIN CONTENT BODY -->
                <!-- BEGIN PAGE HEAD-->
                <div class="page-head">
                    <div class="container-fluid">
                        <!-- BEGIN PAGE TITLE -->
                        <div class="page-title">
                            <h1>{{compnm}} -{{divnm}}- {{dateformat  sdate 'DD/MM/YYYY'}} - {{dateformat edate
                                'DD/MM/YYYY'}} {{usrnm}}
                                <small>
                                    <!-- BEGIN PAGE BREADCRUMBS -->
                                    <ul class="page-breadcrumb breadcrumb">
                                        <li>
                                            <a href="/userright/index">Home</a>
                                            <i class="fa fa-circle"></i>
                                        </li>
                                        <li>
                                            {{!-- Party List --}}
                                        </li>
                                    </ul>
                                    <!-- END PAGE BREADCRUMBS -->
                                </small>
                            </h1>
                        </div>
                        <!-- END PAGE TITLE -->
                    </div>
                </div>
                <!-- END PAGE HEAD-->
                <!-- BEGIN PAGE CONTENT BODY -->
                <div class="page-content">
                    <div class="container-fluid">
                        <!-- BEGIN PAGE CONTENT INNER -->
                        <div class="page-content-inner">
                            <div class="panel panel-primary">
                                <div class="panel-heading">
                                    Godown Wise Opening Stock List
                                    <div class="misc pull-right">
                                        <div class="buttons">
                                            <a href="/stock_opening_module_rm/stock_opening_module_rm_add">
                                                <button class="btn btn-link" type="button"><i class="fa fa-plus"></i>Add / Update
                                                    Godown Wise Opening Stock</button>
                                            </a>
                                        </div>
                                    </div>
                                </div>
                                <div class="panel-body">
                                    <table id="product_detail_table" class="product_detail_table table table-striped table-bordered table-hover">
                                        <thead>
                                            <tr>
                                                {{!-- <th>Action</th> --}}
                                                <th style="width: 0px"></th>
                                                <th>Sno.</th>
                                                <th>Godown Name</th>
                                                <th>Item Group</th>
                                                <th>Sub Quality</th>
                                                <th>Item</th>
                                                <th>Qty</th>
                                           </tr>
                                        </thead>
                                        <tbody>
                                            {{#each MRN as |value key|}}
                                            <tr>
                                                <td></td>
                                                {{!-- <td>
                                                    <div class="btn-group dropdown">
                                                        <button class="btn btn-xs blue dropdown-toggle" type="button"
                                                            data-toggle="dropdown" aria-expanded="false"> Actions
                                                            <i class="fa fa-angle-down"></i>
                                                        </button>
                                                        <ul class="dropdown-menu pull-left" role="menu" style="width:150px">
                                                            <li>
                                                                <a href="javascript:void(0);" class="delbutton delete_stockopingrm"
                                                                    data-id="{{this._id}}">
                                                                    <i class="icon-trash"></i> Delete
                                                                </a>
                                                            </li>
                                                        </ul>
                                                    </div>
                                                </td> --}}
                                                <td style="text-align: center">{{sum @index 1}}</td>
                                                <td style="text-align: center">{{this.gdn_Name.Description}}</td>
                                                <td style="text-align: center">{{this.item_group.Description}}</td>
                                                <td style="text-align: center">{{this.subquality.Description}}</td>
                                                <td style="text-align: center">{{#each value.garu_Aavak_Group as |groupvalue key|}}
                                                    {{#ifCondNot groupvalue.item_Code_Desc null}}{{groupvalue.item_Code_Desc.item_title}}{{/ifCondNot}}
                                                    {{/each}}
                                                </td>
                                                <td style="text-align: right">{{#each value.garu_Aavak_Group as |groupvalue key|}}
                                                    {{#ifCondNot groupvalue.item_Code_Desc null}}{{groupvalue.qntty}}{{/ifCondNot}}
                                                    {{/each}}
                                                </td>
                                            </tr>
                                            {{/each}}
                                        </tbody>
                                        <tfoot>
                                            <tr>
                                                <th class="excludesearch"></th>
                                                {{!-- <th  class="excludesearch">Action</th> --}}
                                                <th>Sno.</th>
                                                <th>Godown Name</th>
                                                <th>Item Group</th>
                                                <th>Sub Quality</th>
                                                <th>Item</th>
                                                <th>Qty</th>
                                            </tr>
                                        </tfoot>
                                    </table>
                                </div>
                            </div>
                        </div>
                        <!-- END PAGE CONTENT INNER -->
                    </div>
                </div>
                <!-- END PAGE CONTENT BODY -->
                <!-- END CONTENT BODY -->
            </div>
            <!-- END CONTENT -->
        </div>
        <!-- END CONTAINER -->
    </div>
</div>
{{> footer_new}}
