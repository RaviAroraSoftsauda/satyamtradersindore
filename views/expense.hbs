{{> header_new}}
{{> top_bar_new}}
{{> menu_new}}
<div class="page-wrapper-row full-height">
    <div class="page-wrapper-middle">
        <!-- BEGIN CONTAINER -->
        <div class="page-container">
            <!-- BEGIN CONTENT -->
            <div class="page-content-wrapper">
                <!-- BEGIN CONTENT BODY -->
                <!-- BEGIN PAGE HEAD-->
                <div class="page-head">
                    <div class="container-fluid">
                        <!-- BEGIN PAGE TITLE -->
                        <div class="page-title">
                            <h1>{{compnm}} -{{divnm}}- {{dateformat  sdate 'DD/MM/YYYY'}} - {{dateformat edate
                                'DD/MM/YYYY'}} {{usrnm}}
                                <small>

                                    <!-- BEGIN PAGE BREADCRUMBS -->
                                    <ul class="page-breadcrumb breadcrumb">
                                        <li>
                                            <a href="/userright/index">Home</a>
                                            <i class="fa fa-circle"></i>
                                        </li>
                                        <li>
                                            Expense
                                        </li>
                                    </ul>
                                    <!-- END PAGE BREADCRUMBS -->
                                </small>
                            </h1>
                        </div>
                        <!-- END PAGE TITLE -->
                    </div>
                </div>
                <!-- END PAGE HEAD-->
                <!-- BEGIN PAGE CONTENT BODY -->
                <div class="page-content">
                    <div class="container-fluid">
                        <!-- BEGIN PAGE CONTENT INNER -->
                        <div class="page-content-inner">
                            <div class="row">
                                <div class="col-md-5">
                                     {{#scuritylogin   security 'Expense Master' 'A'}}
                                    <div class="panel panel-primary">
                                        <div class="panel-heading" id="formheading">
                                            <span>Add</span> Expense
                                        </div>
                                        <div class="panel-body">
                                            <form method="POST" action="/expense/expense_add" enctype="" id="expense"
                                                class="validate">
                                                <div class="row">
                                                    <div class="form-group clearfix">
                                                        <label for="" class="control-label col-md-3">Name</label>
                                                        <div class="col-md-9">
                                                            <input class="form-control expense_name" placeholder="Expense Name"
                                                                name="expense_name" type="text" required>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="form-group clearfix">
                                                        <label for="" class="control-label col-md-3">Type:</label>
                                                        <div class="col-md-9">
                                                            <select class="form-control expense_type" placeholder="Please Select Type"
                                                                name="expense_type" class="" required>
                                                                <option value=""></option>
                                                                <option value="+">+</option>
                                                                <option value="-">-</option>
                                                                <option value="-%">-%</option>
                                                                <option value="-Q">-Q</option>
                                                                <option value="+Q">+Q</option>
                                                                <option value="-W">-W</option>
                                                                <option value="+W">+W</option>
                                                                <option value="-R">-R</option>
                                                                <option value="+R">+R</option>
                                                            </select>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="form-group clearfix">
                                                        <label for="" class="control-label col-md-3"></label>
                                                        <div class="col-md-9">
                                                            <button type="submit" name="save" class="btn btn-success">Submit</button>
                                                            <button type="reset" class="btn btn-primary cancelmaster"
                                                                data-form-ad-action="/expense/expense_add">Cancel</button>
                                                        </div>
                                                    </div>
                                                </div>
                                            </form>
                                        </div>
                                    </div>
                                    {{/scuritylogin}}
                                </div>
                                <div class="col-md-7">
                                    {{#scuritylogin   security 'Expense Master' 'V'}}
                                    <div class="panel panel-primary">
                                        <div class="panel-heading">
                                            Expense List
                                        </div>
                                        <div class="panel-body">
                                            <div class="table-responsive">
                                                <table id="product_detail_table" class="product_detail_table table table-striped table-bordered table-hover">
                                                    <thead>
                                                        <tr>

                                                            <th>Sno.</th>
                                                            <th>Expense Name</th>
                                                            <th>Type</th>
                                                            <th>Action</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        {{#each expense as |value key|}}
                                                        <tr>
                                                            <td>{{sum @index 1}}</td>
                                                            <td>{{this.expense_name}}</td>
                                                            <td>{{this.expense_type}}</td>

                                                            <td>
                                                                <div class="btn-group dropdown">
                                                                    <button class="btn btn-xs blue dropdown-toggle"
                                                                        type="button" data-toggle="dropdown"
                                                                        aria-expanded="false"> Actions
                                                                        <i class="fa fa-angle-down"></i>
                                                                    </button>
                                                                    <ul class="dropdown-menu pull-right" role="menu">
                                                                         {{#scuritylogin   ../security 'Expense Master' 'E'}}
                                                                        <li>
                                                                            <a href="javascript:void(0);" class="edit_expense_btn"
                                                                                data-id="{{this._id}}">
                                                                                <i class="icon-pencil"></i> Edit </a>
                                                                        </li>
                                                                        {{/scuritylogin}}
                                                                        <li class="divider"> </li>
                                                                         {{#scuritylogin   ../security 'Expense Master' 'D'}}
                                                                        <li>
                                                                            <a href="javascript:void(0);" class="delete-expense"
                                                                                data-id="{{this._id}}">
                                                                                <i class="icon-trash"></i> Delete
                                                                            </a>
                                                                        </li>
                                                                        {{/scuritylogin}}
                                                                    </ul>
                                                                </div>
                                                            </td>
                                                        </tr>
                                                        {{/each}}

                                                    </tbody>
                                                    <tfoot>
                                                        <tr>

                                                            <th>Sno.</th>
                                                            <th>Expense Name</th>
                                                            <th>Type</th>
                                                            <th class="excludesearch"></th>
                                                        </tr>
                                                    </tfoot>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                    {{/scuritylogin}}
                                </div>
                            </div>


                        </div>
                        <!-- END PAGE CONTENT INNER -->
                    </div>
                </div>
                <!-- END PAGE CONTENT BODY -->
                <!-- END CONTENT BODY -->
            </div>
            <!-- END CONTENT -->
        </div>
        <!-- END CONTAINER -->
    </div>
</div>
{{> footer_new}}
{{> scripts_new}}