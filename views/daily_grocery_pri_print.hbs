                    <td style="text-align: right; font-size: 14px;" valign="top">{{#each daily_grocery_entry.daily_grocery_entry_group as |groupvalue key|}} {{groupvalue.qty}}</br> {{/each}}</td>
                    <td style="text-align: right; font-size: 14px;" valign="top">{{#each daily_grocery_entry.daily_grocery_entry_group as |groupvalue key|}} {{groupvalue.basic}}</br> {{/each}}</td>
                    <td style="text-align: right; font-size: 14px;" valign="top">{{#each daily_grocery_entry.daily_grocery_entry_group as |groupvalue key|}} {{groupvalue.levy}}</br> {{/each}}</td>
                    <td style="text-align: right; font-size: 14px;" valign="top">{{#each daily_grocery_entry.daily_grocery_entry_group as |groupvalue key|}}--</br> {{/each}}</td>
                    <td style="text-align: right; font-size: 14px;" valign="top">{{#each daily_grocery_entry.daily_grocery_entry_group as |groupvalue key|}}--</br> {{/each}}</td>
                    <td style="text-align: center; font-size: 14px;" valign="top">{{#each daily_grocery_entry.daily_grocery_entry_group as |groupvalue key|}} {{groupvalue.Amt}}&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</br> {{/each}}</td>
                 </tr>
             </tbody>
             <tfoot>
                 <tr >
                     <th></th>
                     <th></th>
                     <th></th>
