                <td style="padding: 0%">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</td>
            </tr>
            <tr>
                <th colspan="5" style="text-align: center; font-size: 12px;"> &nbsp;&nbsp;<b style="font-size: 18px;">{{division.div_mast}}</b></th>
                <th colspan="3" style="text-align: right; padding-right: 4em; font-size: 12px;"> <b style="font-size: 18px;">{{division.groceryMarketRegNo}}&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</b> </th>
                <th colspan="2" style="text-align: left; font-size: 12px;"> <b style="font-size: 18px;">{{dateformat daily_grocery_entry.date "DD/MM/YYYY"}}</b></th>
            </tr>
        </table>
        {{!-- Table Start --}}
        <table  id="groceryPrint"  style="width: 98%">
             <thead >
                <tr >
                    <th style="width: 1em;  height: 9em"></th>
                    <th style="text-align: left; font-size: 15px; ;width: 2em"></th>
                    <th style="text-align: right; font-size: 15px;width: 7em"></th>
                    <th style="text-align: right; font-size: 15px; width: 6em"></th>
                    <th style="text-align: right; font-size: 15px; width: 4em"></th>
