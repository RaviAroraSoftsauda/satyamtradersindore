{{> header_new}}
{{> top_bar_new}}
{{> menu_new}}


<div class="page-wrapper-row full-height">
    <div class="page-wrapper-middle">
        <!-- BEGIN CONTAINER -->
        <div class="page-container">
            <!-- BEGIN CONTENT -->
            <div class="page-content-wrapper">
                <!-- BEGIN CONTENT BODY -->
                <!-- BEGIN PAGE HEAD-->
                <div class="page-head">
                    <div class="container-fluid">
                        <!-- BEGIN PAGE TITLE -->
                        <div class="page-title">
                            <h1>{{compnm}} -{{divnm}}- {{dateformat  sdate 'DD/MM/YYYY'}} - {{dateformat edate
                                'DD/MM/YYYY'}} {{usrnm}}
                                <small>

                                    <!-- BEGIN PAGE BREADCRUMBS -->
                                    <ul class="page-breadcrumb breadcrumb">
                                        <li>
                                            <a href="/userright/index">Home</a>
                                            <i class="fa fa-circle"></i>
                                        </li>
                                        <li>
                                            Party List
                                        </li>
                                    </ul>
                                    <!-- END PAGE BREADCRUMBS -->
                                </small>
                            </h1>
                        </div>
                        <!-- END PAGE TITLE -->
                    </div>
                </div>
                <!-- END PAGE HEAD-->
                <!-- BEGIN PAGE CONTENT BODY -->
                <div class="page-content">
                    <div class="container-fluid">
                        <!-- BEGIN PAGE CONTENT INNER -->
                        <div class="page-content-inner">
                            <div class="row">

                            
                                <div class="col-md-12">
                                    <div class="panel panel-primary">
                                        <div class="panel-heading" id="formheading">
                                            <span>Add</span> SKU
                                        </div>
                                        <div class="panel-body">
                                            {{!-- method="POST" action="/stock_unit_mast/StockUnit_add"  --}}
                                            <form  enctype="" id="stockunit"
                                                class="validate">
                                                <div class="row">
                                                    <div class="form-group clearfix">
                                                        <label for="" class="control-label col-md-3">SKU Name</label>
                                                        <div class="col-md-9">
                                                            <input class="form-control sku_name"required placeholder="SKU Name"
                                                                name="sku_name" type="text" autofocus />
                                                                  <input class="form-control vhpxskuunit"
                                                                name="vhpxskuunit" id="vhpxskuunit" type="hidden" />
                                                        </div>
                                                    </div>
                                                </div>
                                                {{!-- end of row --}}
                                                <div class="row">
                                                    <div class="form-group clearfix">
                                                         <label for="" class="control-label col-md-3">SKU Symbol</label>
                                                        <div class="col-md-9">
                                                            <input class="form-control sku_symbol" placeholder="SKU Symbol"
                                                                name="sku_symbol" type="text" >
                                                        </div>
                                                    </div>
                                                </div>
                                                {{!-- end of row --}}
                                                <div class="row">
                                                    <div class="form-group clearfix">
                                                       <label for="" class="control-label col-md-3">No.Of Decimal</label>
                                                        <div class="col-md-9">
                                                            <input class="form-control no_of_dec" placeholder="No.Of Decimal"
                                                                name="no_of_dec" type="text" >
                                                        </div> 
                                                    </div>
                                                </div>
                                                {{!-- end of row --}}
                                                
                                                {{!-- end of row --}}
                                                <div class="row">
                                                    <div class="form-group clearfix">
                                                        <label for="" class="control-label col-md-3"></label>
                                                        <div class="col-md-9">
                                                            <button type="submit" name="save" class="btn btn-success lock-skumaster">Submit</button>
                                                            <button type="reset" class="btn btn-primary cancelmaster"
                                                                data-form-ad-action="/add_city/city_add">Cancel</button>
                                                        </div>
                                                    </div>
                                                </div>
                                            </form>
                                        </div>
                                    </div>
                                </div>
                                 <div class="col-md-12">
                                    <div class="panel panel-primary">
                                        <div class="panel-heading">
                                            SKU List
                                        </div>
                                        <div class="panel-body">
                                            <div class="table-responsive">
                                                <table id="product_detail_table" class="product_detail_table table table-striped table-bordered table-hover">
                                                    <thead>
                                                        <tr>
                                                            <th>Action</th>
                                                            <th>Sno.</th>
                                                            <th>SKU Name</th>
                                                            <th>SKU Symbol</th>
                                                            <th>No.Of Decimal</th>
                                                           
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        {{#each Stock_Unit as |value key|}}
                                                        <tr>
                                                             <td>
                                                                <div class="btn-group dropdown">
                                                                    <button class="btn btn-xs blue dropdown-toggle"
                                                                        type="button" data-toggle="dropdown"
                                                                        aria-expanded="false"> Actions
                                                                        <i class="fa fa-angle-down"></i>
                                                                    </button>
                                                                    <ul class="dropdown-menu pull-left" role="menu" style="width:150px">
                                                                        <li>
                                                                            <a href="javascript:void(0);" class="su_edit_btn"
                                                                                data-id="{{this._id}}">
                                                                                <i class="icon-pencil"></i> Edit </a>
                                                                        </li>
                                                                        <li class="divider"> </li>
                                                                        <li>
                                                                            <a href="javascript:void(0);" class="su_delete_btn"
                                                                                data-id="{{this._id}}">
                                                                                <i class="icon-trash"></i> Delete
                                                                            </a>
                                                                        </li>
                                                                    </ul>
                                                                </div>
                                                            </td>
                                                            <td>{{sum @index 1}}</td>
                                                            <td>{{this.SKUName}}</td>
                                                            <td>{{this.SKUSymbol}}</td>
                                                            <td>{{this.NoOfDecimal}}</td>
                                                            
                                                        </tr>
                                                        {{/each}}


                                                    </tbody>
                                                    <tfoot>
                                                        <tr>
                                                     <th class="excludesearch"></th>
                                                            <th>Sno.</th>
                                                            <th>SKU Name</th>
                                                            <th>SKU Symbol</th>
                                                            <th>No.Of Decimal</th>
                                                            
                                                        </tr>
                                                    </tfoot>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>


                        </div>
                        <!-- END PAGE CONTENT INNER -->
                    </div>
                </div>
                <!-- END PAGE CONTENT BODY -->
                <!-- END CONTENT BODY -->
            </div>
            <!-- END CONTENT -->
        </div>
        <!-- END CONTAINER -->
    </div>
</div>
{{> footer_new}}
{{> scripts_new}}