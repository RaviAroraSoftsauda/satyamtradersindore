                                <ul class="list-unstyled">
                                   <li><b>{{division.ac_add2}}</b></li>
                                </ul>
                            </div>
                            <div class="col-xs-4" style="margin-top: 32px;"><b style="background-color: gray;">Brokerage Bill</b></div>
                            <div class="col-xs-4 invoice-payment">
                                <ul class="list-unstyled">
                                    <li>
                                        <strong>Mob   :</strong> {{division.ac_mobno}} </li>
                                    <li>
                                        <strong>Email :</strong> {{division.ac_email}} </li>
                                    <li>
                                        <strong>Website:</strong> {{division.ac_wbsite}} </li>
                                </ul>
                            </div>
                        </div>
                        <hr style="border: 1px solid;">
                          <div class="row">
                            <div class="col-xs-4">
                                <ul class="list-unstyled">
                                   <li><strong>Seller Name :</strong> <b>{{trans.pcd.party_name}}</b> </li>
                                   <li><strong></strong> {{trans.pcd.address1}} </li>
                                   <li><strong></strong> <b>{{trans.pcd.city_name.city_name}}</b> </li>
                                </ul>
                            </div>
                             <div class="col-xs-4"></div>
                            <div class="col-xs-4 invoice-payment">
                                <ul class="list-unstyled" style="    border: 1px solid; margin-top: -20px;">
                                    <li><strong>Bill NO:</strong> {{trans.vouc_code}} </li>
                                    <li><strong>Bill Date :</strong>{{dateformat  trans.d_a_t_e 'DD/MM/YYYY'}}</li>
                                    <li><strong>Bill Perioud:</strong><u>{{trans.from_dt}}</u> TO <u>{{trans.to_dt}}</u></li>
                                </ul>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-xs-12">
                                <table border="1px solid" width="100%">
                                    <tr>
                                        <th>Bill Date</th>
                                        <th>Party Name</th>
                                        <th>Con.no</th>
                                        <th>Bill No</th>
                                        <th>Item Name</th>
                                        <th>Bages</th>
                                        <th>Weight</th>
                                        <th>Rate</th>
                                        <th>Brokerage</th>
                                    </tr>
                                    {{#each delveryentry2 as |delveryentry2value key|}}
