                                        <td>{{dateformat  delveryentry2value.sd_date 'DD/MM/YYYY'}}</td>
                                        <td>{{delveryentry2value.pcode.party_name}}</td>
                                        <td>{{delveryentry2value.vouc_code}}</td>
                                        <td>{{delveryentry2value.sauda1.bno}}</td>
                                        <td>{{saudavalue.p_code.product_name}}</td>
                                        <td>{{saudavalue.bag}}</td>
                                        <td>{{saudavalue.wght}}</td>
                                        <td>{{saudavalue.sd_rate}}</td>
                                        <td>{{saudavalue.brbrk_rt}}  {{saudavalue.brbrk_typ}}  {{saudavalue.brbrk_amt}}</td>
                                    </tr>
                                {{/each}}
                                {{/each}}
                                </table>
                                 <div class="row">
                                    <div class="col-xs-8">
                                       <table border="1px solid">
                                        <tr>
                                            <td style="border:none">bag summary</td>
                                            <td></td>
                                            <td>total</td>
                                        </tr>
                                        <tr>
                                            <td>total</td>
                                            <td>{{#saudabagtot delveryentry2}}{{/saudabagtot}}</td>
                                            <td>{{#saudabagtot delveryentry2}}{{/saudabagtot}}</td>
                                        </tr>
                                    </table>
                                    </div>
                                    <div class="col-xs-4 invoice-payment">
                                          <table>
                                        <tr>
                                            <td><b>Total bill amount</b></td>
                                            
                                            <td><b>{{#totbrbrkamt delveryentry2}}{{/totbrbrkamt}}</b></td>
                                        </tr>
                                        <tr>
                                            <td><b>SGST 9%</b></td>
                                            <td></td>
                                        </tr>
                                        <tr>
                                            <td><b>CGST</b></td>
                                            <td></td>
                                        </tr>
                                        <tr>
                                            <td><b>Total bill amount</b></td>
                                            
                                            <td><b>{{#totbrbrkamt delveryentry2}}{{/totbrbrkamt}}</b></td>
                                        </tr>
                                        <tr>
                                            <td style="float: right">(RUPEES ONLY)</td>
                                        </tr>
                                        <tr>
                                            <td>FOR {{division.div_mast}}</td>
                                        </tr>
                                    </table>
                                    </div>          
                        </div>
                        <div class="row">
                            <div class="col-xs-8">
                                <h3>please check the bill carefully & send payment immediately:</h3>
                                <ul class="list-unstyled" style="border:1px solid">
                                    <li><b>{{division.ac_bottline}}</b></li>
                                </ul>
                            </div>
                           
                            <div class="col-xs-4 invoice-payment">
                                <h3>Authorized Signatory</h3>
                            </div>
                        </div>
                                
                            </div>
                        </div>
                        </div>
                        <div class="row">
                            <div class="col-xs-8 invoice-block">
                                
                                <br/>
                                <a class="btn btn-lg blue hidden-print margin-bottom-5" onclick="javascript:window.print();"> Print
                                    <i class="fa fa-print"></i>
                                </a>
                            </div>
                        </div>
                    </div> 
                        </div>
                        <!-- END PAGE CONTENT INNER -->
                    </div>
                </div>
                <!-- END PAGE CONTENT BODY -->
                <!-- END CONTENT BODY -->
            </div>
            <!-- END CONTENT -->
        </div>
